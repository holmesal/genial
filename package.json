{
  "name": "relay-starter-kit",
  "private": true,
  "description": "A quick way to get up and running with Relay",
  "repository": "facebook/relay-starter-kit",
  "version": "0.1.0",
  "scripts": {
    "start": "forego start -f Procfile_dev",
    "build-server": "babel src -d dist",
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "watch": "npm-watch",
    "update-schema": "babel-node ./scripts/updateSchema.js"
  },
  "watch": {
    "update-schema": "src/schema.js",
    "build-server": "src/**/*.js"
  },
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "6.3.21",
    "babel-loader": "6.2.0",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-relay-plugin": "0.6.3",
    "classnames": "2.2.1",
    "connect-history-api-fallback": "^1.1.0",
    "express": "4.13.3",
    "express-graphql": "0.4.5",
    "faker": "^3.0.1",
    "graphql": "0.4.14",
    "graphql-relay": "0.3.6",
    "lodash": "^4.2.1",
    "material-ui": "^0.14.4",
    "pg": "^4.4.4",
    "pg-hstore": "^2.3.2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-relay": "^0.6.1",
    "react-router": "^2.0.0",
    "react-router-relay": "^0.9.0",
    "react-tap-event-plugin": "^0.2.2",
    "sequelize": "^3.19.2",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0"
  },
  "devDependencies": {
    "npm-watch": "^0.1.1"
  }
}
